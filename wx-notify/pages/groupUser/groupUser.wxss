page {
  background-color: #f2eeee;
}

.warn-view {
  width: 730rpx;
  padding: 10rpx;
  background-color: orange;
  font-size: 25rpx;
  position: fixed;
  top: 0;
  text-align: center;
}

.title-view {
  top: 58rpx;
  display: flex;
  position: fixed;
  width: 100%;
  background-color: white;
}

.title-view text {
  text-align: center;
  flex: 1;
}

.nickname-title {
  background-color: #1296db;
}

.groupUser-wrap {
  margin-top: 116rpx;
  width: 100%;
  background-color: white;
  font-size: 30rpx;
}

.groupUser-item {
  width: 100%;
  height: 60rpx;
  display: flex;
  align-items: center;
  border-bottom: 2rpx solid #f2eeee;
}

.groupUser-item .group {
  width: 50%;
  text-align: center;
}

.groupUser-item .name {
  width: 38%;
  text-align: center;
  border-left: 4rpx solid #f2eeee;
}

.delete-box {
  position: absolute;
  right: 10rpx;
  width: 80rpx;
  height: 60rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.delete-box image {
  height: 40rpx;
  width: 40rpx;
}
.order-section{
  display: flex;
  width: 100%;
  position: fixed;
  height: 100rpx;
  bottom: 0;
}
.order-section button{
  border-radius: 0;
  flex: 1;
}